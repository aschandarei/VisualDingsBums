<Window x:Class="VisualDingsBums.STDOUTView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="STDOUT"
        Height="300"
        Width="300"
        Closing="Window_Closing"
        Icon="..\IMAGES\BF.ico"
        ShowInTaskbar="False">
    <Grid>
        <TextBox Text="{Binding STDOUT, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Window}}" />
    </Grid>
</Window>
